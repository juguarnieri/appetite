{
  "expo": {
    "name": "Appetite",
    "slug": "appetite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/talher.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/talher.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appetite.app",
      "splash": {
        "image": "./assets/talher.png",
        "resizeMode": "contain"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.appetite.app",
      "splash": {
        "image": "./assets/talher.png",
        "resizeMode": "contain"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/talher.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/talher.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Appetite to access your photos.",
          "cameraPermission": "Allow Appetite to access your camera."
        }
      ]
    ]
  }
}
